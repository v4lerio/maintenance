{
    "name": "modularsoftware/maintenance",
    "description": "Modular Maintenance is a work in progress for property services. SPA based on Enso SPA (Laravel 5.6, Vue and Bulma)",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "~4.1",
        "guzzlehttp/guzzle": "6.3.3",
        "laravel-enso/addressesmanager": "2.3.25",
        "laravel-enso/charts": "2.3.12",
        "laravel-enso/commentsmanager": "2.3.26",
        "laravel-enso/contacts": "2.2.32",
        "laravel-enso/core": "2.10.34",
        "laravel-enso/dataimport": "2.2.48",
        "laravel-enso/documentsmanager": "2.2.29",
        "laravel-enso/howtovideos": "2.0.11",
        "laravel-enso/notifications": "2.1.26",
        "laravel-enso/versioning": "1.1.10",
        "laravel/framework": "5.6.39",
        "laravel/horizon": "1.4.3",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "0.8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.1",
        "codedungeon/phpunit-result-printer": "0.19.14",
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "1.8.0",
        "laracasts/generators": "dev-develop",
        "laravel-enso/testhelper": "1.4.13",
        "mockery/mockery": "1.2.0",
        "nunomaduro/collision": "2.1.1",
        "phpunit/phpunit": "7.5.3",
        "wnx/laravel-stats": "1.8.0"
    },
    "repositories": [{
        "type": "git",
        "url": "https://github.com/bogdankharchenko/Laravel-5-Generators-Extended"
    }],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --force --tag='enso-assets'",
            "php artisan vendor:publish --tag='localisation-lang-files'",
            "php artisan localisation:merge"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
